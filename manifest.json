{
    "manifest_version": 3,
    "name": "Search AI",
    "version": "0.0.1",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "assets/icon-16.png",
            "48": "assets/icon-48.png",
            "128": "assets/icon-128.png"
        }
    },
    "icons": {
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "background": {
        "service_worker": "src/background.tsx"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/content.tsx"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}